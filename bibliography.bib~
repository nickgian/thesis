@TechReport{core_erlang_specs,
  author = {Richard Carlsson and Bj{\"o}rn Gustavsson and Erik Johansson 
            and Thomas Lindgren and Sven-Olof Nystr{\"o}m and Mikael 
            Pettersson and Robert Virding},
  title = {Core {E}rlang 1.0 language specification},
  institution = {Department of Information Technology, Uppsala University},
  department = {Computing Science Division},
  year = {2000},
  number = {2000-030},
  month = nov,
  abstract = {We describe a core language for the concurrent functional
              language Erlang, aptly named ``Core Erlang'', presenting
              its grammar and informal static and dynamic semantics
              relative to Erlang. We also discuss built-in functions and
              other open issues, and sketch a syntax tree representation.}
}

